{
  "name": "r-matter-certis",
  "version": "0.1.0",
  "description": "rMatterCertis - E-commerce Product Crawling Application",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
  "tauri": "OS_ACTIVITY_MODE=disable CA_DEBUG_TRANSACTIONS=0 tauri",
  "tauri:dev": "OS_ACTIVITY_MODE=disable CA_DEBUG_TRANSACTIONS=0 tauri dev",
  "tauri:dev:nosave": "OS_ACTIVITY_MODE=disable CA_DEBUG_TRANSACTIONS=0 MC_SKIP_DB_SAVE=1 tauri dev",
  "dev:nosave": "MC_SKIP_DB_SAVE=1 vite & OS_ACTIVITY_MODE=disable CA_DEBUG_TRANSACTIONS=0 MC_SKIP_DB_SAVE=1 tauri dev",
  "tauri:dev:legacy-actor": "OS_ACTIVITY_MODE=disable CA_DEBUG_TRANSACTIONS=0 MC_FEATURE_STAGE_EXECUTOR_TEMPLATE=0 tauri dev",
  "tauri:dev:template": "OS_ACTIVITY_MODE=disable CA_DEBUG_TRANSACTIONS=0 MC_FEATURE_STAGE_EXECUTOR_TEMPLATE=1 tauri dev",
    "tauri:build": "OS_ACTIVITY_MODE=disable tauri build",
  "type-check": "tsc --noEmit",
  "verify:runtime": "bash scripts/verify_runtime_plan.sh src-tauri/target/debug/logs/back_front.log",
  "dev:run-and-verify": "bash scripts/run_and_verify.sh",
  "lint:rust": "bash scripts/lint_rust.sh",
  "lint:rust:strict": "cargo fmt --manifest-path src-tauri/Cargo.toml --all -- --check && cargo clippy --manifest-path src-tauri/Cargo.toml --all-targets -- -D warnings",
  "verify:fast": "npm run verify:runtime || DISABLE_STRUCTURED_CHECKS=1 bash scripts/verify_runtime_plan.sh src-tauri/target/debug/logs/back_front.log",
  "precommit": "bash scripts/precommit.sh",
  "audit:grep": "bash scripts/grep_audit.sh",
  "archive:backups": "bash scripts/archive_backups.sh",
  "db:dedup-urls": "bash scripts/sqlite_dedup_by_url.sh",
  "check:list": "node scripts/check_csa_list_pages.mjs"
  },
  "license": "MIT",
  "dependencies": {
    "@tauri-apps/api": "^2.6.0",
    "@tauri-apps/plugin-dialog": "^2.3.0",
    "@tauri-apps/plugin-opener": "^2",
    "@types/three": "^0.178.1",
    "chart.js": "^4.5.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "d3": "^7.9.0",
    "d3-force-3d": "^3.0.6",
    "date-fns": "^4.1.0",
    "react-chartjs-2": "^5.3.0",
    "solid-chartjs": "^1.3.11",
    "solid-js": "^1.9.3",
    "three": "^0.178.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@tauri-apps/cli": "^2",
    "@types/d3": "^7.4.3",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.6.2",
    "vite": "^6.0.3",
    "vite-plugin-solid": "^2.11.0"
  }
}
